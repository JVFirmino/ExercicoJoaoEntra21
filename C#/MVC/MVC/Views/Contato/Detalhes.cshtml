@model Contato

@{
    ViewData["Title"] = "Detalhes";
}

<h1>@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-4">
        <form>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <p>@Model.Id</p>
            </div>
            <div class="form-group">
                <p>@Model.Name</p>
            </div>
            <div class="form-group">
                <p>@Model.Email</p>
            </div>
            <div class="form-group">
                <p>@Model.Fone</p>
            </div>
            <div class="form-group">
                <a asp-action="Index">Back to List</a>
            </div>
        </form>
    </div>
</div>
 
